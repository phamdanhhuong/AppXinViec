<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background-->
    <Color x:Key="primaryBackColor1">#ffffff</Color>
    <Color x:Key="primaryBackColor2">#0f9f49</Color>
    <Color x:Key="secondaryBackColor1">#e8edf2</Color>
    <Color x:Key="secondaryBackColor2">#223241</Color>
    <!--Border-->
    <Color x:Key="winBorderColor">#0f9f49</Color>

    <!--Font Colors-->
    <SolidColorBrush x:Key="plainTextColor1" Color="Black"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="White"/>
    <SolidColorBrush x:Key="textGreen">#3dc47a</SolidColorBrush>
    <SolidColorBrush x:Key="textLightGray">#b0a89e</SolidColorBrush>
    <SolidColorBrush x:Key="textBlack">#3f3f48</SolidColorBrush>
    <SolidColorBrush x:Key="textWhite">#fafeff</SolidColorBrush>
    <SolidColorBrush x:Key="textLime(UCCreateCV)">#529631</SolidColorBrush>
    <!--Panel-->
    <SolidColorBrush x:Key="panelActiveColor" Color="#12f2e2"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#17ffb2"/>
    <SolidColorBrush x:Key="panelColor1" Color="#0f9f49"/>
    <SolidColorBrush x:Key="panelColor2" Color="#223241"/>
    <SolidColorBrush x:Key="panelUCJobs">#f2fbf6</SolidColorBrush>
    <SolidColorBrush x:Key="stBackgroundColor">#f3f5f7</SolidColorBrush>
    <SolidColorBrush x:Key="scBackgroundColor">#ffffff</SolidColorBrush>
</ResourceDictionary>